    <data>

        <variable
            name="viewModel"
            type="co.yap.sendmoney.home.main.SMBeneficiaryParentViewModel" />

        <import type="android.view.View" />

        <import type="co.yap.sendmoney.R" />
    </data>

    <LinearLayout
